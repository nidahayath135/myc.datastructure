#include <stdio.h>
#include <stdlib.h>

char* my_strdup(char *src) {
    int len = 0;
    while (src[len]) len++;

    char *copy = (char*)malloc(len + 1);
    if (!copy) return NULL;

    for (int i = 0; i <= len; i++)
        *(copy + i) = *(src + i);

    return copy;
}

int main() {
    char s[] = "Hello Nida";
    char *dup = my_strdup(s);
    printf("Copied string: %s\n", dup);
    free(dup);
    return 0;
}
