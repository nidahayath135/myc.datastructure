#include <stdio.h>
#include <stdlib.h>
int main() {
    int *ptr;
    ptr = (int*)malloc(2 * sizeof(int)); 

    ptr[0] = 5;
    ptr[1] = 10;

    ptr = (int*)realloc(ptr, 4 * sizeof(int)); 
    ptr[2] = 15;
    ptr[3] = 20;

    for (int i = 0; i < 4; i++)
        printf("%d ", ptr[i]);

    free(ptr);
    return 0;
}
